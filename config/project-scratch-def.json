{
    "orgName": "Dentelo",
    "edition": "Enterprise",
    "features": ["ContactsToMultipleAccounts", 
        "Communities",
        "HealthCloudUser", 
        "LightningSalesConsole", 
        "LightningServiceConsole", 
        "PersonAccounts", 
        "ProcessBuilder",
        "ProductsAndSchedules",
        "RecordTypes",
        "SalesUser",
        "ServiceCloud",
        "ServiceUser",
